{
  "plugins": [],
  "source": {
    "exclude": ["node_modules", ".vscode"],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "recurse": true,
    "encoding": "utf8",
    "destination": "./jsdocs",
    "template": "node_modules/docdash"
  }
}
